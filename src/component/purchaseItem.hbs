<div class="row">
    {{#each itemList}}
    <div class="col-12">
        <div class="row">
            <div class="col-4">
                {{number}}
                <h6>{{status}}</h6>
            </div>
            <div class="col-2">
                {{#with amount}}
                {{title}}:
                <h4>{{value}}</h4>
                {{/with}}
            </div>
            <div class="col-4">
                {{#each images}}
                <img class="card-img" style="max-width: 60px;" src="{{this}}">
                {{/each}}
            </div>
            <div class="col-2">
                {{#with button}}
                {{#if isPrimary}}
                <a href="{{link}}" class="btn btn-primary">
                    {{text}}
                    <i class="fa-solid fa-angles-right" style="color: #ffffff;"></i>
                </a>
                {{/if}}
                {{/with}}
            </div>
            <hr>
        </div>
    </div>
    {{/each}}
</div>